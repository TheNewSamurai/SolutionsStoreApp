<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @partner.first %></h1>
    <%= link_to "Edit", :action => :edit, :id => @partner.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">First</td>
        <td class="itemValue"><%= @partner.first%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Last</td>
        <td class="itemValue"><%= @partner.last%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Address</td>
        <td class="itemValue"><%= @partner.address%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">City</td>
        <td class="itemValue"><%= @partner.city%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">State</td>
        <td class="itemValue"><%= @partner.state%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Lat</td>
        <td class="itemValue"><%= @partner.lat%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Long</td>
        <td class="itemValue"><%= @partner.long%></td>
      </tr>
      
    </table>
  </div>

</div>
