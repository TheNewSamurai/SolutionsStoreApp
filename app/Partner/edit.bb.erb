<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @partner.object %>
    <h1>Edit <%= @partner.first%></h1>
    <%= link_to "Delete", :action => :delete, :id => @partner.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @partner.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">First: </td>
              <td class="itemValue"><input type="text" name="partner[first]" value="<%= @partner.first %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Last: </td>
              <td class="itemValue"><input type="text" name="partner[last]" value="<%= @partner.last %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Address: </td>
              <td class="itemValue"><input type="text" name="partner[address]" value="<%= @partner.address %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">City: </td>
              <td class="itemValue"><input type="text" name="partner[city]" value="<%= @partner.city %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">State: </td>
              <td class="itemValue"><input type="text" name="partner[state]" value="<%= @partner.state %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Lat: </td>
              <td class="itemValue"><input type="text" name="partner[lat]" value="<%= @partner.lat %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Long: </td>
              <td class="itemValue"><input type="text" name="partner[long]" value="<%= @partner.long %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
