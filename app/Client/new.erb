<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @client.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @client.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="client[first]" class="fieldLabel">First</label>
            <input type="text" id="client[first]" name="client[first]" <%= placeholder("First") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="client[last]" class="fieldLabel">Last</label>
            <input type="text" id="client[last]" name="client[last]" <%= placeholder("Last") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="client[phone]" class="fieldLabel">Phone</label>
            <input type="text" id="client[phone]" name="client[phone]" <%= placeholder("Phone") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="client[address]" class="fieldLabel">Address</label>
            <input type="text" id="client[address]" name="client[address]" <%= placeholder("Address") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="client[city]" class="fieldLabel">City</label>
            <input type="text" id="client[city]" name="client[city]" <%= placeholder("City") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="client[state]" class="fieldLabel">State</label>
            <input type="text" id="client[state]" name="client[state]" <%= placeholder("State") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
